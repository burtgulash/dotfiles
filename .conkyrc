out_to_x no
own_window no
out_to_console yes
background no
max_text_width 0
update_interval 2.0
total_run_times 0
short_units yes
if_up_strictness address
use_spacer left
override_utf8_locale no
cpu_avg_samples 1

TEXT
[{"full_text": " uptime [$uptime_short]", "color": "\#ffffff"}\
,{"full_text": " NET"}\
,{"full_text":
    ${if_up eth0}
        "[${addr eth0}]"\
    ${else}
    ${if_up wlp5s0}
        "[${wireless_essid wlp5s0} ${addr wlp5s0}]"\
    ${else}
    ${if_up wlp2s0}
        "[${wireless_essid wlp2s0} ${addr wlp2s0}]"\
    ${else}
    ${if_up mlan0}
        "[${wireless_essid mlan0} ${addr mlan0}]"\
    ${else}
    ${if_up wlan0}
        "[${wireless_essid wlan0} ${addr wlan0}]"\
    ${else}
        "disconnected"\
    ${endif}
    ${endif}
    ${endif}
    ${endif}
    ${endif}
}\
,{"full_text": " CPU [${cpu}% $machine]", "color": "\#ffffff"}\
,{"full_text": " RAM [${memperc}% swp ${swapperc}%]", "color": ${if_match ${memperc}<90} "\#ffffff" ${else} "\#ff0000" ${endif}}\
,{"full_text": " BAT [${battery_short BAT0}]"}\
,{"full_text": " VOL [${exec amixer get Master -M | grep -oP '\d*%' | head -n1}]", "color": "\#ffffff"}\
,{"full_text": " ${time %d.%m.%Y %H:%M}", "color": "\#ffffff"}],
